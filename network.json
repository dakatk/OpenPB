{
    "cost": "mse",
    "layers": [
        {
            "neurons": 4,
            "activation": "relu"
        },
        {
            "neurons": 6,
            "dropout": 0.2,
            "activation": "sigmoid"
        },
        {
            "neurons": 4,
            "activation": "leakyrelu"
        }
    ],
    "optimizer": {
        "name": "sgd",
        "learning_rate": 0.05
    },
    "metric": {
        "name": "accuracy",
        "args": {
            "epsilon": 0.2
        }
    },
    "epochs": 10000,
    "batch_size": 2
}
