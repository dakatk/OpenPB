{
    "cost": "mse",
    "layers": [
        {
            "neurons": 4,
            "activation": "relu"
        },
        {
            "neurons": 6,
            "dropout_rate": 0.2,
            "activation": "sigmoid"
        },
        {
            "neurons": 4,
            "activation": "leakyrelu"
        }
    ],
    "optimizer": {
        "name": "sgd",
        "learning_rate": 0.01
    },
    "encoder": {
        "name": "one_hot",
        "args": {
            "max": 3
        }
    },
    "metric": {
        "name": "accuracy",
        "args": {
            "min": 1.0
        }
    },
    "epochs": 10000
}
